fun sayHi(first, last) {
  print "Ola, " + first + " " + last + "!";
}

sayHi("Saulo", "de Tassio / Maria Helena");

// Teste de retorno e recursão com Fibonacci
fun fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

var start = clock();
var resultadoFib = fib(10);
var end = clock();

print "Fibonacci de 10 eh:";
print resultadoFib; // Deve imprimir 55
print "Calculo do Fibonacci levou:";
print end - start;